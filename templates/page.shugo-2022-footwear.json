    "f9e75828-29be-453f-a8f0-932fe5c28e76": {
      "type": "project-text-block",
      "settings": {
        "text-block-title": "",
        "text-block-text": "<p>SHUGO’s ORIGINS Collection dropped publicly in November of 2022. The ZERO Cleat and ZERO TR serve as the cornerstone of the collection, featuring limited-run colorways inspired by Stroman’s most-celebrated moments and milestones in life. <\/p>",
        "background": "#ffffff"
      }
    },
